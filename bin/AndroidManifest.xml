<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maxxxx.ippconverter"
    android:versionCode="1"
    android:versionName="1.5.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Home"
            android:label="@string/title_activity_home" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="IPPConverter" android:label="Infix to Prefix/Postfix"></activity>
        <activity android:name="PIPConverter" android:label="Prefix to Postfix/Infix"></activity>
        <activity android:name="PPIConverter" android:label="Postfix to Prefix/Infix"></activity>
        <activity android:label="Error" android:name="Error" android:theme="@android:style/Theme.DeviceDefault.Light.Dialog"></activity>
    </application>

</manifest>